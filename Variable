#TYPES OF VARIABLE
#integer
Age = 25

#float
weight = 62.5

#string
God = krishna

#Variables in strings

Day = 18
Month = september
Temp = 25

print(f"Today is {Month} {Day} and its {temp} outside")

#Boolean
the_dog_is_playing = True

if the_dog_is_playing:
 print("the dog is playing")
else:
 print("the dog is not playing")
